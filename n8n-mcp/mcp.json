{
    "mcpServers": {
      "n8n-mcp": {
        "command": "npx",
        "args": ["n8n-mcp"],
        "env": {
          "MCP_MODE": "stdio",
          "LOG_LEVEL": "error",
          "DISABLE_CONSOLE_OUTPUT": "true",
          "N8N_API_URL": "http://localhost:5678", //replace with your n8n api url
          "N8N_API_KEY": "N8N_API_KEY" //replace with your n8n api key
        }
      }
    }
  }